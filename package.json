{
  "name": "jozi-makert-be",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "test": "jest --forceExit --detectOpenHandles",
    "vercel-build": "npm run build",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "format": "biome format --write src/",
    "seed:features": "ts-node -r tsconfig-paths/register src/database/seed-features.ts",
    "seed:plans": "ts-node -r tsconfig-paths/register src/database/seed-plans.ts",
    "seed:categories": "ts-node -r tsconfig-paths/register src/database/seed-categories-attributes.ts",
    "seed:users-vendors-products": "ts-node -r tsconfig-paths/register src/database/seed-users-vendors-products.ts",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev"
  },
